<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DemoCRUDnet7.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DemoCRUDnet7"
    xmlns:views="clr-namespace:DemoCRUDnet7.Views">

    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
                    Title="Liste des Photos"
                    ContentTemplate="{DataTemplate views:PhotoListPage}"
                    Route="PhotoList"/>
    </TabBar>

</Shell>
