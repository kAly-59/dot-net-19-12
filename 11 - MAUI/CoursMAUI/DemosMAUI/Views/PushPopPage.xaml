<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DemosMAUI.Views.PushPopPage"
             Title="PushPopPage">
    <VerticalStackLayout Spacing="10">
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Entry x:Name="EtText"/>
        <Button Text="Push nouvelle PushPopPage" Clicked="OnPushClicked"/>
        <Button Text="Pop Page Actuelle" Clicked="OnPopClicked"/>
        <Button Text="Pop to Root" Clicked="OnPopAllClicked"/>
    </VerticalStackLayout>
</ContentPage>